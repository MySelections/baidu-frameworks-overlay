<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="33.33%p"
    android:horizontalGap="@dimen/password_keyboard_horizontalGap"
    android:verticalGap="@dimen/password_keyboard_verticalGap"
    android:keyHeight="@dimen/password_keyboard_key_height_numeric"
    >

    <Row android:rowEdgeFlags="top">
        <Key android:codes="49" android:keyIcon="@drawable/pin_1_normal"
             android:keyEdgeFlags="left"/>
        <Key android:codes="50" android:keyIcon="@drawable/pin_2_normal"/>
        <Key android:codes="51" android:keyIcon="@drawable/pin_3_normal"
             android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="52" android:keyIcon="@drawable/pin_4_normal"
             android:keyEdgeFlags="left"/>
        <Key android:codes="53" android:keyIcon="@drawable/pin_5_normal"/>
        <Key android:codes="54" android:keyIcon="@drawable/pin_6_normal"
             android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="55" android:keyIcon="@drawable/pin_7_normal"
             android:keyEdgeFlags="left"/>
        <Key android:codes="56" android:keyIcon="@drawable/pin_8_normal"/>
        <Key android:codes="57" android:keyIcon="@drawable/pin_9_normal"
             android:keyEdgeFlags="right"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="48" android:keyIcon="@drawable/pin_0_normal"
             android:keyEdgeFlags="left"/>
        <Key android:codes="10" android:keyIcon="@drawable/pin_ok_normal"
             android:keyWidth="66.66%p" android:keyEdgeFlags="right"/>
    </Row>

</Keyboard>
