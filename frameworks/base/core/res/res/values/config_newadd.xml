<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- for volume key wakeup, disable default
         set it to true in device overlay when needed -->
    <add-resource type="integer" name="config_wakeup_by_volume_key"/>
    <bool name="config_wakeup_by_volume_key">false</bool>

    <!-- Hardware 'face' keys present on the device, stored as a bit field.
         This integer should equal the sum of the corresponding value for each
         of the following keys present:
             1 - Home
             2 - Back
             4 - Menu
             8 - Assistant (search)
            16 - App switch
         For example, a device with Home, Back and Menu keys would set this
         config to 7. -->
    <add-resource type="integer" name="config_deviceHardwareKeys"/>
    <integer name="config_deviceHardwareKeys">0</integer>
    <java-symbol type="integer" name="config_deviceHardwareKeys"/>

    <add-resource type="integer" name="config_automatic_rotate_delay"/>
    <integer name="config_automatic_rotate_delay">2</integer>
    <add-resource type="bool" name="config_quickbootSupport"/>
    <bool name="config_quickbootSupport">true</bool>
    <add-resource type="bool" name="config_poweron_by_twice_press"/>
    <bool name="config_poweron_by_twice_press">false</bool>
    <add-resource type="bool" name="config_close_button_backlight"/>
    <bool name="config_close_button_backlight">false</bool>

    <add-resource type="string" name="config_customCameraPackage"/>
    <string name="config_customCameraPackage"></string>
    <add-resource type="string" name="config_customFmRadioPackage"/>
    <string name="config_customFmRadioPackage"></string>
</resources>
